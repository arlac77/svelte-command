{
  "name": "svelte-command",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "svelte": "./src/index.svelte",
  "description": "general command execution handling for svelte components",
  "keywords": [
    "commands",
    "component",
    "svelte",
    "vite",
    "web"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "prepare": "vite build",
    "start": "rollup -c tests/app/rollup.config.mjs -w && vite dev",
    "test": "npm run test:ava && npm run test:cafe",
    "test:cafe": "testcafe $BROWSER:headless tests/cafe/*.js -s build/test --page-request-timeout 9000 --app-init-delay 3000 --app \"rollup -c tests/app/rollup.config.mjs -w\"",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs",
    "build": "rollup -c tests/app/rollup.config.mjs",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-virtual": "^2.1.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.49",
    "ava": "^4.3.0",
    "documentation": "^13.2.5",
    "mf-styling": "^1.2.22",
    "rollup": "^2.75.7",
    "rollup-plugin-dev": "^2.0.0",
    "semantic-release": "^19.0.3",
    "svelte": "^3.48.0",
    "testcafe": "^1.19.0",
    "vite": "^2.9.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/svelte-command.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/svelte-command/issues"
  },
  "homepage": "https://github.com/arlac77/svelte-command#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-ava",
      "arlac77/template-cloudflare",
      "arlac77/template-svelte-component#next"
    ]
  }
}
