{
  "name": "svelte-command",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "svelte": "./src/index.svelte",
  "keywords": [
    "component",
    "web",
    "svelte"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "start": "rollup -c tests/app/rollup.config.mjs -w",
    "test": "npm run test:cafe",
    "test:cafe": "testcafe $BROWSER:headless tests/cafe/*.js -s build/test --app-init-delay 1000 --app \"rollup -c tests/app/rollup.config.mjs -w\"",
    "docs": "documentation readme ./src/index.svelte --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/index.svelte"
  },
  "devDependencies": {
    "semantic-release": "^17.3.3",
    "builtin-modules": "^3.1.0",
    "rollup": "^2.36.2",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-typescript": "^8.1.0",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-virtual": "^2.0.3",
    "@rollup/plugin-inject": "^4.0.2",
    "rollup-plugin-executable": "^1.6.2",
    "rollup-plugin-native": "^1.2.14",
    "rollup-plugin-consts": "^1.0.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-dev": "^1.1.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-gzip": "2.5.0",
    "testcafe": "^1.10.1",
    "rollup-plugin-svelte": "^7.0.0",
    "svelte": "^3.31.2",
    "rollup-plugin-postcss": "^4.0.0",
    "postcss-import": "^14.0.0",
    "postcss": "^8.2.3",
    "documentation": "github:arlac77/documentation"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/svelte-command.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/svelte-command/issues"
  },
  "homepage": "https://github.com/arlac77/svelte-command#readme"
}